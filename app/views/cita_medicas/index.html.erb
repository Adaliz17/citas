<p id="notice"><%= notice %></p>

<h1>Cita Medicas</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Doctor</th>
      <th>Persona</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cita_medicas.each do |cita_medica| %>
      <tr>
        <td><%= cita_medica.fecha %></td>
        <td><%= cita_medica.doctor %></td>
        <td><%= cita_medica.persona %></td>
        <td><%= link_to 'Show', cita_medica %></td>
        <td><%= link_to 'Edit', edit_cita_medica_path(cita_medica) %></td>
        <td><%= link_to 'Destroy', cita_medica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cita Medica', new_cita_medica_path %>
